{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-redis": "*",
        "64robots/nova-fields": "^0.22.0",
        "ampeco/nova-date-range-filter": "^0.1.3",
        "ebess/advanced-nova-media-library": "^3.6",
        "emilianotisato/nova-tinymce": "^1.2",
        "eminiarts/nova-tabs": "^1.4",
        "flagstudio/custom-card": "*",
        "flagstudio/nova-actions-left": "^0.2.0",
        "flagstudio/nova-admin-bar": "^0.4.0",
        "flagstudio/nova-contacts": "*",
        "flagstudio/nova-instructions": "*",
        "fourstacks/nova-repeatable-fields": "^1.3",
        "fruitcake/laravel-cors": "^2.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "jenssegers/date": "^4.0",
        "laravel/framework": "^8.75",
        "laravel/helpers": "^1.4",
        "laravel/nova": "^3.30.0",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.2",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "meilisearch/meilisearch-php": "^0.19.1",
        "php-ffmpeg/php-ffmpeg": "^0.18.0",
        "spatie/data-transfer-object": "^2.8",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-backup": "^7.7",
        "spatie/laravel-enum": "^2.5",
        "spatie/laravel-image-optimizer": "^1.6",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-missing-page-redirector": "^2.7",
        "spatie/laravel-model-states": "^2.1",
        "spatie/laravel-permission": "^4.1",
        "spatie/laravel-query-builder": "^3.4",
        "spatie/laravel-sitemap": "^6.0",
        "spatie/laravel-sluggable": "^3.1",
        "spatie/laravel-tail": "^4.3",
        "spatie/laravel-view-models": "^1.3",
        "spatie/schema-org": "^3.4",
        "whitecube/nova-flexible-content": "^0.2.8",
        "fakerphp/faker": "^1.9.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-dump-server": "^1.7",
        "facade/ignition": "^2.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "roave/security-advisories": "dev-latest",
        "laravel/telescope": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "mkdir -p .git/hooks && cp tools/git-hooks/pre-commit .git/hooks/pre-commit"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "csfix": "vendor/bin/php-cs-fixer fix . --config=/var/www/.php-cs-fixer.dist.php --path-mode=intersection --using-cache=no --allow-risky=yes",
        "csfix-validate": "vendor/bin/php-cs-fixer fix . --config=/var/www/.php-cs-fixer.dist.php --path-mode=intersection --using-cache=no --allow-risky=yes --dry-run -v",
        "autotests": "vendor/bin/phpunit --testdox --do-not-cache-result"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaContacts"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaInstructions"
        },
        {
            "type": "path",
            "url": "./nova-components/CustomCard"
        }
    ]
}
